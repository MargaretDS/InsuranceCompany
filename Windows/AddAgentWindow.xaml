<Window x:Class="InsuranceCompany.Windows.AddAgentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InsuranceCompany.Windows"
        mc:Ignorable="d"
        Title="Добавить страховщика" Height="300" Width="350" MinHeight="300" MinWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="ФИО:" Grid.Row="0" Grid.Column="0"></Label>
        <Label Content="Паспорт:" Grid.Row="1" Grid.Column="0"></Label>
        <TextBox x:Name="TxtFIO" Grid.Row="0" Grid.Column="1"></TextBox>
        <TextBox x:Name="TxtPassport" Grid.Row="1" Grid.Column="1"></TextBox>
        <Button x:Name="BtnAddAgent" Content="Добавить" Grid.Row="2" Grid.ColumnSpan="2" Click="BtnAddAgent_Click"></Button>
    </Grid>
</Window>
